- content_for :title, title(t('head.title.admin_snippets'))

- content_for :pagination do
  = render :partial => 'shared/pagination', :locals => {:items => @snippets}

.manage-actions
  = link_to t('common.add'), new_admin_snippet_path, :class => 'button'

table.manage-items
  tbody
    tr.table-header
      th = t('common.ident')
      th = t('common.name')
      th = t('common.created_at')
      th
      th
    = render @snippets